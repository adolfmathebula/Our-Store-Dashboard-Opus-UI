{
    "name": "our-store-dashboard",
    "description": "",
    "version": "1.0.0",
    "keywords": [
        "opus-ui",
        "example",
        "opus"
    ],
    "scripts": {
        "start": "concurrently --kill-others \"npm run dev -- --host\" \"nodemon node_modules/@intenda/opus-ui-packager/src/packager.js\"",
        "dev": "vite --port 3000",
        "build-json": "node node_modules/@intenda/opus-ui-packager/src/packager.js",
        "build": "vite build"
    },
    "dependencies": {
        "@intenda/opus-ui": "^1.2.1",
        "@intenda/opus-ui-components": "^1.1.2",
        "@intenda/opus-ui-grid": "^1.1.2",
        "@intenda/opus-ui-repeater-grid": "^0.3.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@intenda/opus-ui-packager": "^1.2.0",
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@vitejs/plugin-react": "^4.2.1",
        "concurrently": "^8.2.2",
        "eslint": "^8.55.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "nodemon": "^3.1.0",
        "vite": "^5.0.8",
        "vite-plugin-dynamic-import": "^1.5.0"
    },
    "nodemonConfig": {
        "ignore": [],
        "watch": [
            "app"
        ],
        "ext": "js,json,md",
        "ignoreRoot": [],
        "delay": 100
    }
}